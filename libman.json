{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "alpinejs@3.14.1",
      "destination": "public/lib/alpinejs",
      "files": ["cdn.min.js"]
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "public/lib/bootstrap",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "library": "font-awesome@6.6.0",
      "destination": "public/lib/font-awesome",
      "files": ["css/all.min.css", "js/all.min.js", "sprites/*", "webfonts/*"]
    },
    {
      "library": "htmx@2.0.0",
      "destination": "public/lib/htmx",
      "files": ["htmx.min.js"]
    },
    {
      "provider": "unpkg",
      "library": "hyperscript.org@0.9.12",
      "destination": "public/lib/hyperscript.org",
      "files": ["dist/_hyperscript.min.js"]
    },
    {
      "library": "sweetalert2@11.14.0",
      "destination": "public/lib/sweetalert2",
      "files": ["sweetalert2.min.css", "sweetalert2.all.min.js"]
    }
  ]
}
