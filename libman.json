{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "alpinejs@3.14.1",
      "destination": "src/public/lib/alpinejs",
      "files": ["cdn.min.js"]
    },
    {
      "library": "font-awesome@6.6.0",
      "destination": "src/public/lib/font-awesome",
      "files": ["css/all.min.css", "js/all.min.js", "sprites/*", "webfonts/*"]
    },
    {
      "library": "htmx@2.0.2",
      "destination": "src/public/lib/htmx",
      "files": ["htmx.min.js"]
    },
    {
      "provider": "unpkg",
      "library": "hyperscript.org@0.9.12",
      "destination": "src/public/lib/hyperscript.org",
      "files": ["dist/_hyperscript.min.js"]
    },
    {
      "library": "sweetalert2@11.14.0",
      "destination": "src/public/lib/sweetalert2",
      "files": ["sweetalert2.min.css", "sweetalert2.all.min.js"]
    }
  ]
}
